from locust import Locust, TaskSet, task

class MyTaskSet(TaskSet):

    def setup(self):
        print("executing setup")
        
    @task
    def my_task(self):
        print("Locust instance (%r) executing my_task" % (self.locust))
        time.sleep(3)
        
    def teardown(self):
        print("executing teardown")

class MyLocust(Locust):
    task_set = MyTaskSet
